{
    "openapi": "3.0.0",
    "info": {
        "title": "API for BnB service",
        "version": "1.0.1"
    },
    "servers": [
        {
            "url": "/",
            "description": "Main endpoint"
        }
    ],
    "paths": {
        "/api/bookables/{bookable}/availability": {
            "get": {
                "tags": [
                    "Booking"
                ],
                "summary": "Check abilities date for booking",
                "operationId": "293525a94bee202cffe6118f47e50c83",
                "parameters": [
                    {
                        "$ref": "#/components/parameters/bookable"
                    },
                    {
                        "$ref": "#/components/parameters/start"
                    },
                    {
                        "$ref": "#/components/parameters/end"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Dates are not available for booking. If the list is empty, then the booking is available",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/BookingAvailabilityResource"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/bookables/categories": {
            "get": {
                "tags": [
                    "Bookable"
                ],
                "summary": "Get bookable categories",
                "operationId": "d1927c0410e4c4406bdd5b52a4c29f86",
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/BookableCategoriesResource"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/bookables": {
            "get": {
                "tags": [
                    "Bookable"
                ],
                "summary": "Show bookable list by filter and paginator",
                "operationId": "d64f80e35e4a71265f1055227b2d8227",
                "parameters": [
                    {
                        "$ref": "#/components/parameters/bookableCategoryId"
                    },
                    {
                        "$ref": "#/components/parameters/priceMin"
                    },
                    {
                        "$ref": "#/components/parameters/priceMax"
                    },
                    {
                        "$ref": "#/components/parameters/priceWeekendMin"
                    },
                    {
                        "$ref": "#/components/parameters/priceWeekendMax"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "allOf": [
                                        {
                                            "$ref": "#/components/schemas/BookableIndexResource"
                                        },
                                        {
                                            "$ref": "#/components/schemas/PaginateShort"
                                        },
                                        {
                                            "$ref": "#/components/schemas/PaginateMeta"
                                        }
                                    ]
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation error for input query parameters",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/bookables/{bookable}": {
            "get": {
                "tags": [
                    "Bookable"
                ],
                "summary": "Show bookable by bookable id",
                "operationId": "26fb3ceb7a77b34e03cfba8fa809d7f5",
                "parameters": [
                    {
                        "$ref": "#/components/parameters/bookable"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/BookableShowResource"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Not found bookable by id",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Message"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/bookables/{bookable}/reviews": {
            "get": {
                "tags": [
                    "Review"
                ],
                "summary": "Get bookable list review with pagination",
                "operationId": "20c7ad5d45a6f5c98078f8eccf6875bd",
                "parameters": [
                    {
                        "$ref": "#/components/parameters/bookable"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "allOf": [
                                        {
                                            "$ref": "#/components/schemas/BookableReviewResource"
                                        },
                                        {
                                            "$ref": "#/components/schemas/PaginateShort"
                                        },
                                        {
                                            "$ref": "#/components/schemas/PaginateMeta"
                                        }
                                    ]
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Not found bookable by id",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Message"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/booking-by-review/{reviewKey}": {
            "get": {
                "tags": [
                    "Review"
                ],
                "summary": "Get data for write review",
                "operationId": "83fe101dd2fd5151c29ca2bc38cd606d",
                "parameters": [
                    {
                        "name": "reviewKey",
                        "in": "path",
                        "schema": {
                            "description": "Review key",
                            "type": "string",
                            "format": "uuid"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/BookingByReviewKeyResource"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Not owner review",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Message"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Not found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Message"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/api/user": {
            "get": {
                "tags": [
                    "Auth",
                    "User"
                ],
                "summary": "Get info about authenticated user",
                "operationId": "4e44cf9a540963d379e34df9bf78481c",
                "responses": {
                    "201": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/FetchUserResource"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthenticated",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Message"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Auth user and generating cookie header.",
                "operationId": "bdf09eecbdc94e71cbd77eeea63ba325",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/EmailPassword"
                            }
                        }
                    }
                },
                "responses": {
                    "422": {
                        "description": "These credentials do not match our records",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Message"
                                }
                            }
                        }
                    },
                    "200": {
                        "description": "Success auth",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/logout": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Logoff user. Invalidate auth cookie.",
                "operationId": "47e830778114b69f85de7aec2912702a",
                "responses": {
                    "204": {
                        "description": "Logout success",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/sanctum/csrf-cookie": {
            "get": {
                "tags": [
                    "Auth"
                ],
                "summary": "Refresh CSRF cookie (app protection app)",
                "operationId": "5ce50283bd16af5f2d2f80b3a73b0327",
                "responses": {
                    "204": {
                        "description": "Get new cookie",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/forgot-password": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Emailed link for reset password",
                "operationId": "96dfdf387e941aaa80796834e2f0617c",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "email": {
                                        "$ref": "#/components/schemas/EmailPassword/properties/email"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "422": {
                        "description": "Validation error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ValidationError"
                                }
                            }
                        }
                    },
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Message"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/register": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Register new user",
                "description": "After register user application emailed verification link. Firstly account is unverified.",
                "operationId": "6b566abaed3e4207964a225b1d3258a8",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "allOf": [
                                    {
                                        "$ref": "#/components/schemas/EmailPassword"
                                    },
                                    {
                                        "properties": {
                                            "first_name": {
                                                "title": "user name",
                                                "type": "string",
                                                "minimum": 2
                                            },
                                            "last_name": {
                                                "title": "user last name",
                                                "type": "string",
                                                "minimum": 2
                                            },
                                            "password_confirmation": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "422": {
                        "description": "Validation error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ValidationError"
                                }
                            }
                        }
                    },
                    "201": {
                        "description": "Success register",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "BookableByReviewKeyResource": {
                "properties": {
                    "id": {
                        "description": "Bookable id",
                        "type": "string",
                        "format": "uuid"
                    },
                    "category": {
                        "description": "Bookable category name",
                        "type": "string"
                    },
                    "title": {
                        "description": "Bookable name",
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "BookableCategoriesResource": {
                "title": "Show available bookable category",
                "properties": {
                    "data": {
                        "type": "array",
                        "items": {
                            "properties": {
                                "id": {
                                    "type": "string",
                                    "format": "uuid"
                                },
                                "name": {
                                    "type": "string"
                                }
                            },
                            "type": "object"
                        },
                        "minItems": 0
                    }
                },
                "type": "object"
            },
            "BookableIndexResource": {
                "title": "Bookable item",
                "properties": {
                    "data": {
                        "type": "array",
                        "items": {
                            "properties": {
                                "id": {
                                    "type": "string",
                                    "format": "uuid"
                                },
                                "title": {
                                    "type": "string"
                                },
                                "description": {
                                    "description": "Short description",
                                    "type": "string",
                                    "maximum": 80
                                },
                                "price": {
                                    "description": "Regular price per day",
                                    "type": "integer",
                                    "minimum": 1
                                },
                                "price_weekend": {
                                    "description": "Weekend price per day",
                                    "type": "integer",
                                    "minimum": 1
                                },
                                "category": {
                                    "description": "Bookable category",
                                    "type": "string"
                                }
                            },
                            "type": "object"
                        },
                        "minItems": 0
                    }
                },
                "type": "object"
            },
            "BookableReviewResource": {
                "title": "Review item",
                "properties": {
                    "data": {
                        "type": "array",
                        "items": {
                            "properties": {
                                "id": {
                                    "type": "string",
                                    "format": "uuid"
                                },
                                "description": {
                                    "description": "Review description",
                                    "type": "string"
                                },
                                "rating": {
                                    "description": "Bookable rating",
                                    "type": "integer",
                                    "maximum": 5,
                                    "minimum": 1
                                },
                                "createdAt": {
                                    "description": "Review created date",
                                    "type": "string",
                                    "format": "date"
                                },
                                "user": {
                                    "oneOf": [
                                        {
                                            "$ref": "#/components/schemas/UserReviewResource"
                                        }
                                    ],
                                    "nullable": true
                                }
                            },
                            "type": "object"
                        },
                        "minItems": 0
                    }
                },
                "type": "object"
            },
            "BookableShowResource": {
                "properties": {
                    "data": {
                        "type": "array",
                        "items": {
                            "properties": {
                                "id": {
                                    "type": "string",
                                    "format": "uuid"
                                },
                                "title": {
                                    "type": "string"
                                },
                                "description": {
                                    "description": "Short description",
                                    "type": "string"
                                },
                                "price": {
                                    "description": "Regular price per day",
                                    "type": "integer",
                                    "minimum": 1
                                },
                                "price_weekend": {
                                    "description": "Weekend price per day",
                                    "type": "integer",
                                    "minimum": 1
                                },
                                "category": {
                                    "description": "Bookable category",
                                    "type": "string"
                                }
                            },
                            "type": "object"
                        },
                        "maxItems": 1,
                        "minItems": 1
                    }
                },
                "type": "object"
            },
            "BookingAvailabilityResource": {
                "description": "Items with dates are not available",
                "properties": {
                    "data": {
                        "type": "array",
                        "items": {
                            "properties": {
                                "start": {
                                    "type": "string",
                                    "format": "date"
                                },
                                "end": {
                                    "type": "string",
                                    "format": "date"
                                }
                            },
                            "type": "object"
                        },
                        "minItems": 0
                    }
                },
                "type": "object"
            },
            "BookingByReviewKeyResource": {
                "properties": {
                    "data": {
                        "properties": {
                            "id": {
                                "description": "Booking id",
                                "type": "string",
                                "format": "uuid"
                            },
                            "start": {
                                "description": "Booking start date",
                                "type": "string",
                                "format": "data"
                            },
                            "end": {
                                "description": "Booking end date",
                                "type": "string",
                                "format": "data"
                            },
                            "user": {
                                "$ref": "#/components/schemas/ReviewerResource"
                            },
                            "bookable": {
                                "$ref": "#/components/schemas/BookableByReviewKeyResource"
                            }
                        },
                        "type": "object"
                    }
                },
                "type": "object"
            },
            "FetchUserResource": {
                "title": "Info about authenticated user",
                "properties": {
                    "email": {
                        "type": "string",
                        "format": "email"
                    },
                    "name": {
                        "description": "Full user name",
                        "type": "string"
                    },
                    "newReviewCount": {
                        "description": "Count of reviews waite user comment",
                        "type": "integer"
                    },
                    "isVerified": {
                        "description": "User account is verified",
                        "type": "boolean"
                    }
                },
                "type": "object"
            },
            "ReviewerResource": {
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "email": {
                        "type": "string",
                        "format": "email"
                    },
                    "name": {
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "UserReviewResource": {
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "name": {
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "Message": {
                "title": "Message",
                "properties": {
                    "message": {
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "EmailPassword": {
                "properties": {
                    "email": {
                        "type": "string",
                        "format": "email"
                    },
                    "password": {
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "PaginateMeta": {
                "properties": {
                    "meta": {
                        "properties": {
                            "current_page": {
                                "type": "integer",
                                "example": 1
                            },
                            "from": {
                                "type": "integer",
                                "example": 1
                            },
                            "to": {
                                "type": "integer",
                                "example": 12
                            },
                            "last_page": {
                                "type": "integer",
                                "example": 9
                            },
                            "path": {
                                "type": "string",
                                "format": "url",
                                "example": "http://localhost/api/bookables"
                            },
                            "per_page": {
                                "type": "integer",
                                "example": 12
                            },
                            "total": {
                                "type": "integer",
                                "example": 100
                            },
                            "links": {
                                "type": "array",
                                "items": {
                                    "properties": {
                                        "active": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "label": {
                                            "type": "string",
                                            "example": "1"
                                        },
                                        "url": {
                                            "type": "string",
                                            "format": "url",
                                            "example": "http://localhost/api/bookables?page=1",
                                            "nullable": true
                                        }
                                    },
                                    "type": "object"
                                },
                                "minItems": 1
                            }
                        },
                        "type": "object"
                    }
                },
                "type": "object"
            },
            "PaginateShort": {
                "properties": {
                    "links": {
                        "properties": {
                            "first": {
                                "type": "string",
                                "format": "url",
                                "example": "http://localhost/api/bookables?page=1"
                            },
                            "last": {
                                "type": "string",
                                "format": "url",
                                "example": "http://localhost/api/bookables?page=9"
                            },
                            "next": {
                                "type": "string",
                                "format": "url",
                                "example": "http://localhost/api/bookables?page=2",
                                "nullable": true
                            },
                            "prev": {
                                "type": "string",
                                "format": "url",
                                "example": null,
                                "nullable": true
                            }
                        },
                        "type": "object"
                    }
                },
                "type": "object"
            },
            "ValidationError": {
                "properties": {
                    "message": {
                        "description": "Main validation error",
                        "type": "string"
                    },
                    "errors": {
                        "properties": {
                            "fieldName": {
                                "title": "All error for field name (field key)",
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            }
                        },
                        "type": "object"
                    }
                },
                "type": "object"
            }
        },
        "parameters": {
            "bookable": {
                "name": "bookable",
                "in": "path",
                "required": true,
                "schema": {
                    "title": "Bookable uuid",
                    "type": "string",
                    "format": "uuid"
                }
            },
            "bookableCategoryId": {
                "name": "bookableCategoryId",
                "in": "path",
                "required": false,
                "schema": {
                    "title": "Bookable category",
                    "type": "integer"
                }
            },
            "priceMin": {
                "name": "priceMin",
                "in": "path",
                "required": false,
                "schema": {
                    "title": "Bookable min regular price per pay",
                    "type": "integer"
                }
            },
            "priceMax": {
                "name": "priceMax",
                "in": "path",
                "required": false,
                "schema": {
                    "title": "Bookable max regular price per pay",
                    "type": "integer"
                }
            },
            "priceWeekendMin": {
                "name": "priceWeekendMin",
                "in": "path",
                "required": false,
                "schema": {
                    "title": "Bookable min weekend price per pay",
                    "type": "integer"
                }
            },
            "priceWeekendMax": {
                "name": "priceWeekendMax",
                "in": "path",
                "required": false,
                "schema": {
                    "title": "Bookable max weekend price per pay",
                    "type": "integer"
                }
            },
            "start": {
                "name": "start",
                "in": "query",
                "required": true,
                "schema": {
                    "title": "Start date for check ability booking",
                    "type": "string",
                    "format": "date",
                    "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
                }
            },
            "end": {
                "name": "end",
                "in": "query",
                "required": true,
                "schema": {
                    "title": "End date for check ability booking",
                    "type": "string",
                    "format": "date",
                    "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
                }
            }
        },
        "securitySchemes": {
            "sanctum": {
                "type": "apiKey",
                "description": "Authentication in application base on session authorization with  Laravel\\Fortify.\n                  response set Cookie to client.",
                "name": "XSRF-TOKEN",
                "in": "cookie"
            }
        }
    }
}